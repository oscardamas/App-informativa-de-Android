<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dm.oscar.appdms">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_lobo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActivityOne"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActivityAcercaDe"
            android:screenOrientation="portrait" />

        <intent-filter>
            <action android:name="android.intent.action.ACTIVITYONE" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity
            android:name=".ActivityContacto"
            android:screenOrientation="portrait" />

        <intent-filter>
            <action android:name="android.intent.action.ACTIVITYCONTACTO" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity
            android:name=".ActivityConvocatorias"
            android:screenOrientation="portrait" />

        <intent-filter>
            <action android:name="android.intent.action.ACTIVITYCONVOCATORIAS" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".ActivityNoticias"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.ACTYVITYNOTICAS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
    </application>

</manifest>