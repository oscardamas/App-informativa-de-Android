<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dm.oscar.appdms">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityOne">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityAcercaDe" />

        <intent-filter>
            <action android:name="android.intent.action.ACTIVITYONE" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".ActivityContacto" />

        <intent-filter>
            <action android:name="android.intent.action.ACTIVITYCONTACTO" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".ActivityNoticias" />
        <intent-filter>
            <action android:name="android.intent.action.ACTIVITYNOTICIAS" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

    </application>

</manifest>