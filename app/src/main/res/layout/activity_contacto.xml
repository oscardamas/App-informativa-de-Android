<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="dm.oscar.appdms.ActivityContacto">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contactese con AppDMS"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="28dp"
        android:textColor="@color/colorPrimaryDark"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Si desea recibir personalmente mas informacion atravez de:"
        android:id="@+id/textView4"
        android:textSize="20dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Correo electronico:"
        android:id="@+id/textView5"
        android:textSize="18dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ejemplo@dominio.com"
        android:id="@+id/textView6"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:editable="true"
        android:textIsSelectable="true"
        android:autoLink="email" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefono: 9:00-17:00"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp"
        android:textSize="18dp"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="999-999-9999"
        android:id="@+id/textView8"
        android:textSize="18dp"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView3"
        android:phoneNumber="true"
        android:password="false"
        android:editable="true"
        android:textIsSelectable="true"
        android:autoLink="phone" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Paginas web oficiales:"
        android:id="@+id/textView9"
        android:textSize="18dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_marginTop="42dp"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="www.ejemplo1.com.mx"
        android:id="@+id/textView10"
        android:textSize="18dp"
        android:layout_below="@+id/textView9"
        android:textIsSelectable="true"
        android:editable="true"
        android:textColorLink="#2abfec"
        android:autoLink="web" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="www.ejemplo2.gob.mx"
        android:id="@+id/textView11"
        android:textSize="18dp"
        android:layout_below="@+id/textView10"
        android:layout_alignEnd="@+id/textView10"
        android:textIsSelectable="true"
        android:editable="true"
        android:textColorLink="#2abfec"
        android:autoLink="web" />
</RelativeLayout>
